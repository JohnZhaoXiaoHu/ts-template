{
  "name": "ts-template",
  "version": "0.0.0",
  "description": "TypeScript Porject Template.",
  "main": "dist/index.js",
  "directories": {
    "doc": "docs",
    "example": "src/demo",
    "test": "src/test"
  },
  "scripts": {
    "build": "tsc",
    "debug": "node -r ts-node/register --inspect-brk src/main/index.ts",
    "dist": "node dist",
    "start": "npm run build && npm run dist",
    "test": "node --inspect-brk node_modules/jasmine/bin/jasmine.js --config=jasmine.json --reporter=jasmine-console-reporter --require=ts-node/register"
  },
  "keywords": [
    "typescript"
  ],
  "author": "IInfinity <I.INF@Outlook.com> (https://don.red/)",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@iinfinity/logger": "^0.2.0",
    "typescript": "^3.3.4000"
  },
  "devDependencies": {
    "@types/jasmine": "^3.3.12",
    "@types/node": "^11.12.1",
    "jasmine": "^3.3.1",
    "jasmine-console-reporter": "^3.1.0",
    "ts-node": "^8.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DevinDon/ts-template.git"
  },
  "bugs": {
    "url": "https://github.com/DevinDon/ts-template/issues"
  },
  "homepage": "https://github.com/DevinDon/ts-template"
}
